/* pages/liushui/liushui.wxss */
@import "../fuyong/fuyong.wxss";
page{
  background: #F2F2F2;
}
.liushui_ul{
  width: 100%;
}
.liushui_li{
  width: 100%;
  height: 157rpx;
  padding: 33rpx 32rpx 24rpx 34rpx;
  box-sizing: border-box;
  margin-bottom: 16rpx;
  background: white;
  
}
.liu_top{
  display: flex;
  justify-content: space-between;
}
.liu_top text:nth-of-type(1){
  font-size: 34rpx;
color: #404040;
letter-spacing: 1.84rpx;
}
.liu_top text:nth-of-type(2){
  font-size: 32rpx;
color: #EA4B34;
}
.liu_bott{
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
}
.liu_bott text:nth-of-type(1){
  font-size: 30rpx;
color: #A1A1A1;
letter-spacing: 1.84rpx;
}
.liu_bott text:nth-of-type(2){
  font-size: 32rpx;
color: #404040;
}
.xuanfu{
  position: fixed;
  bottom: 300rpx;
  right: 50rpx;
  width: 128rpx;
  height: 128rpx;
}
.wu_xianlu{
  width: 596rpx;
  height: 516rpx;
  display: block;
  margin: 100rpx auto;
}
.baise{
  width: 100%;
  height: 100%;
  position: fixed;
  background: white;
  z-index: -10;
  top: 0;
  left: 0;
}