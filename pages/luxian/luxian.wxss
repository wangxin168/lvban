/* pages/luxian/luxian.wxss */
/* @import "../../wxParse/wxParse.wxss"; */
@import '../../towxml/style/main.wxss';
@import "../fuyong/fuyong.wxss";
.banner {
  width: 100%;
  height: 376rpx;
  margin-top: 30rpx;
}
view{
  overflow: visible;
}
swiper {
  width: 100%;
  height: 100%;
}

swiper-item {
  width: 100%;
  height: 100%;
}

swiper-item image {
  width: 717rpx;
  height: 100%;
  display: block;
  margin: auto;
  border-radius: 10rpx;
}

.xianlu {
  padding: 0 16rpx 0 24rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}

.xianlu .xianlu_tit {
  font-size: 36rpx;
  color: #404040;
}

.xianlu image {
  width: 45rpx;
  height: 45rpx;
  margin-right: 68rpx;
}

.xianlu_id {
  font-size: 30rpx;
  color: #898989;
}
.xianlu button{
  font-size: 26rpx;
  color: #B8B8B8;
  border:none !important;
  border-radius:0;
  line-height: normal;
  background: none;
  margin: 0;
  padding: 0;
}
button::after{
  border: none;
}
.xianlu_and {
  font-size: 26rpx;
  color: #b8b8b8;
}

.xianlu_go {
  font-size: 30rpx;
  color: #9c9c9c;
}

.yungaold {
  width: 730rpx;
  height: 100rpx;
  margin-left: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 2rpx solid #ececec;
  padding: 0 12rpx 0 0;
  box-sizing: border-box;
}

.yungaold .old {
  font-size: 30rpx;
  color: #898989;
}

.yungaold .much {
  font-size: 38rpx;
  color: #d34c4c;
  margin-left: 25rpx;
}

.yungaold .jifen {
  font-size: 30rpx;
  color: #9c9c9c;
}

.yungaold .jifen text {
  color: #EA4B34;
  width: 100rpx;
  display: inline-block;
  font-family: "Helvetica";
}

.xian {
  width: 100%;
  height: 22rpx;
  background: #f2f2f2;
}

/* 推荐路线 */

.route_line {
  padding: 0 0 0 20rpx;
  width: 100%;
  box-sizing: border-box;
  line-height: 90rpx;
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.route_line image{
  width: 262rpx;
  height: 54rpx;
  display: block;
}
.route_line_pre {
  padding-left: 20rpx;
  position: relative;
  font-size: 38rpx;
  color: #404040;
}

.route_line_pre ::before {
  content: '';
  display: block;
  width: 8rpx;
  height: 43rpx;
  background: #eb5139;
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
}

.xianlu_con {
  width: 100%;
  /* margin: 12rpx auto 200rpx; */
  font-size: 30rpx;
  color: #7a7a7a;
  line-height: 47rpx;
  margin-bottom: 200rpx;
}

.foot {
  width: 100%;
  height: 110rpx;
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  background: white;
}
.foot_krfu{
  background: none!important;
  padding: 0;
  line-height: 1.3!important;
  border-radius: 0!important;
  border: none!important;
}
.foot .foot_left {
  text-align: center;
  border-top: 2rpx solid #d8d8d8;
  width: 237rpx;
}

.foot .foot_left image {
  width: 48rpx;
  height: 50rpx;
  margin-top: 6rpx;
}

.foot .foot_left view {
  font-size: 28rpx;
  color: #6c6c6c;
  text-align: center;
}

.foot .foot_right {
  width: 513rpx;
  text-align: center;
  line-height: 110rpx;
  font-size: 32rpx;
  color: #fff;
  background: #ea4b34;
  border-radius: 0!important;
}

