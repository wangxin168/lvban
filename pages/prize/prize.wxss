/* pages/prize/prize.wxss */
@import "../fuyong/fuyong.wxss";
page{
  width:100%;
  height: 100%;
  /* position: fixed; */
  /* left: 0; */
  /* top:0; */
  /* right: 0; */
  /* bottom: 0; */
  background: #F2F2F2;
  /* z-index: -111; */
}
.prize_header{
  position: relative;
  color: #fff;
  height:256rpx;
}
.prize_img{
  width: 100%;
  height:256rpx;
}
.prize_num{
  font-size: 60rpx;
  position: absolute;
  left:47rpx;
  top:60rpx;
}
.prize_text{
  font-size: 30rpx;
  position: absolute;
  left:47rpx;
  top:144rpx;
}
.prize_list_item{
  display: flex;
  padding: 40rpx;
  width: 100%;
  box-sizing: border-box;
  justify-content: space-between;
  background: #fff;
  margin-bottom: 16rpx;
}
.prize_title{
  font-size: 34rpx;
  color: #404040;
}
.prize_stu{
  display: inline-block;
  width:133rpx;
  height: 40rpx;
  font-size: 26rpx;
  color: #EA4B34;
  border-radius: 20rpx;
  background: #FFCCC4;
  text-align: center;
  line-height: 40rpx;
  margin-left: 20rpx;
}
.prize_nick{
  font-size: 28rpx;
  color: #A1A1A1;
  margin: 20rpx 0;
}
.prize_time{
  font-size: 30rpx;
  color: #A1A1A1;
}
.prize_list_num{
  margin-top: 50rpx;
  font-size: 40rpx;
  color: #404040;
}
.prize_list_num text{
  font-size: 30rpx;
  color: #404040;
  padding-left: 10rpx;
}
.xuanfu{
  position: fixed;
  bottom: 300rpx;
  right: 50rpx;
  width: 128rpx;
  height: 128rpx;
}
.wu_xianlu{
  width: 596rpx;
  height: 516rpx;
  display: block;
  margin: 100rpx auto;
}
.baise{
  width: 100%;
  height: 100%;
  position: fixed;
  background: white;
  z-index: -10;
  top: 0;
  left: 0;
}
