/* pages/lvyou/lvyou.wxss */
@import "../fuyong/fuyong.wxss";
.tui_ul{
  width: 96%;
  display: flex;
  justify-content: space-between;
  margin: 20rpx auto;
  flex-wrap: wrap;
}
.tui_ul .tui_li{
  width: 49%;
  height: 609rpx;
  border-radius: 6rpx;
  position: relative;
  margin-bottom: 17rpx;
  box-shadow: 0 6rpx 5rpx 0 rgba(225,225,225,0.50);
}
.tui_ul .tui_li .tui_img{ 
  width: 100%;
  height: 374rpx;
}
.tui_ul .tui_li .tui_tit{
  font-size: 32rpx;
  color: #565656;
  margin-left: 13rpx;
}
.tui_ul .tui_li .tui_lu{
  font-size: 28rpx;
  color: #898989;
  margin-left: 13rpx;
  margin-top: 5rpx;
}
.tui_ul .tui_li .tui_jian{
  width: 236rpx;
  height: 40rpx;
  background-image: linear-gradient(-90deg, #FE6165 0%, #FAA76C 100%);
  border-radius: 6rpx;
  font-size: 26rpx;
color: #FFFFFF;
margin-left: 13rpx;
margin-top: 3rpx;
text-align: center;
line-height: 40rpx;
}
.tui_ul .tui_li .tui_price{
  font-size: 23rpx;
color: #D34C4C;
display: flex;
align-items: center;
margin-left: 13rpx;
margin-top: 8rpx;
}
.tui_ul .tui_li .tui_price .price_da{
  font-size: 40rpx;
color: #D34C4C;
}
.tui_ul .tui_li .tui_chu{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  background: rgba(0, 0, 0, 0.6);
  font-size: 26rpx;
  color: #FFFFFF;
  height: 43rpx;
  line-height: 43rpx;
  padding: 0 10rpx;
}
.xuanfu{
  position: fixed;
  bottom: 300rpx;
  right: 50rpx;
  width: 128rpx;
  height: 128rpx;
}
.wu_xianlu{
  width: 596rpx;
  height: 516rpx;
  display: block;
  margin: auto;
}